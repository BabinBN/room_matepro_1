<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:tnt="sap.tnt"
  xmlns:f="sap.f"
  height="100%"
  controllerName="roommatepro.controller.dashboard">

  <App>
    <Page id="myPage" title="Room Mate Dashboard" enableScrolling="true">
      
      <!-- Header -->
      <customHeader>
        <OverflowToolbar>
          <Button icon="sap-icon://menu2" press=".onCollapseExpandPress" />
          <Image src="images/log.png" height="30px" />
          <Title text="Room Mate Pro" level="H2" />
          <ToolbarSpacer />
          <Button icon="sap-icon://search" />
          <Button icon="sap-icon://bell" />
          <Avatar initials="SN" displaySize="XS" />
        </OverflowToolbar>
      </customHeader>

      <!-- Main Layout -->
      <content>
        <HBox fitContainer="true">
          
          <!-- Side Navigation -->
         
           <tnt:SideNavigation id="sideNavigation" selectedKey="walked" expanded="false" width="250px">
        <tnt:NavigationList>
          <tnt:NavigationListItem text="Home" icon="sap-icon://home" press="onHomePress" />
  <tnt:NavigationListItem text="Vegetables" icon="sap-icon://nutrition-activity" press="onVegetablesCardPress" />
  <tnt:NavigationListItem text="Groceries" icon="sap-icon://cart" press="onGroceriesCardPress" />
  <tnt:NavigationListItem text="Room Members" icon="sap-icon://collaborate" press="onRoomMembersPress" />
  <tnt:NavigationListItem text="Others" icon="sap-icon://thing-type" press="onOthersPress" />
 </tnt:NavigationList>
              <tnt:fixedItem>
                <tnt:NavigationList>
                  <tnt:NavigationListItem text="Settings" icon="sap-icon://action-settings" />
                </tnt:NavigationList>
              </tnt:fixedItem>
            </tnt:SideNavigation>
       
              <HBox class="sapUiSmallMargin" justifyContent="Start" >
              <mvc:XMLView viewName="roommatepro.view.dashboardtiles" />
            </HBox>
        </HBox>
      </content>
    </Page>
  </App>
</mvc:View>
